    "@octokit/core": "^4.2.0",
    "@types/jest": "^28.1.8",
    "commander": "^10.0.0",
    "dotenv": "^16.0.3",
    "figlet": "^1.5.2",
    "jest": "^28.1.3",
    "node-fetch": "^2.6.9",
    "parse-github-url": "^1.0.2",
    "ts-jest": "^28.0.8"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/parse-github-url": "^1.0.0",
    "typescript": "^4.9.4"
  }
}
